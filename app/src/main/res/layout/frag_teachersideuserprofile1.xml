<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<RelativeLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:focusable="true"
    android:background="@color/white"
    android:id="@+id/student_profile"
   >

    <RelativeLayout
        android:id="@+id/header"
        android:layout_width="match_parent"
        android:layout_height="150dp">


        <ImageView
            android:id="@+id/top_img"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/welcome_text"
            android:layout_marginLeft="-280dp"
            android:layout_marginRight="70dp"
            android:scaleType="centerCrop"
            android:src="@drawable/top_img" />
        <ImageView
            android:id="@+id/im_back"
            android:layout_width="30dp"
            android:layout_height="40dp"
            android:src="@mipmap/ic_arrow_left_white_24dp"
            android:tint="@color/black"
            android:paddingTop="5dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="5dp"/>

        <TextView
            android:id="@+id/tv_heading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/top_img"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="60dp"
            android:gravity="center"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:text="Home"
            android:textColor="@color/black"
            android:textSize="20dp" />



    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/header"
        android:layout_marginLeft="-30dp"
        android:layout_marginTop="-60dp"
        android:layout_marginRight="-30dp"

        >


        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scaleType="centerCrop"
            android:src="@mipmap/background_cross" />


    </RelativeLayout>




    <RelativeLayout
        android:id="@+id/card"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/header"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/recangle_shape_blue">


        <TextView
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="60dp"
            android:padding="3dp"
            android:text="Priya Joshi"
            android:textColor="@color/black"
            android:textSize="18dp"
            android:layout_marginBottom="40dp"
            android:textStyle="bold"

            />




    </RelativeLayout>
    <!--            </android.support.v7.widget.CardView>-->

    <RelativeLayout
        android:id="@+id/profile"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignTop="@id/card"
        android:background="@color/white"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="-60dp">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/im_profile"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:src="@drawable/noimage"
            app:civ_border_color="#bce7e5"
            app:civ_border_width="7dp" />
    </RelativeLayout>














